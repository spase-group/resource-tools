            <xsd:documentation xml:lang="en">
               Identifiers for regions of the star upon which
               our solar system is centered.
            </xsd:documentation>
         </xsd:annotation>
      <xsd:restriction base="xsd:string">
<xsd:enumeration value="Chromosphere">
